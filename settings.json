{
    "workbench.colorTheme": "Theme",
    "workbench.iconTheme": "material-icon-theme",

    // Using the light mode
    "indentRainbow.indicatorStyle": "light",
    // we use a simple 1 pixel wide line
    "indentRainbow.lightIndicatorStyleLineWidth": 1,
    // the same colors as above but more visible
    "indentRainbow.colors": [
        "rgba(255,255,64,0.3)",
        "rgba(127,255,127,0.3)",
        "rgba(255,127,255,0.3)",
        "rgba(79,236,236,0.3)"
    ],

    "background.fullscreen": {
          "image": "https://wallpapercave.com/wp/wp7472828.jpg", // url of your image
          // "image": ["https://pathtoimage.png"], // An array may be useful when set interval for carousel
          "opacity": 0.99, // 0.85 ~ 0.95 recommended
          "size": "cover", // also css, `cover` to self-adaption (recommended)，or `contain`、`200px 200px`
          "position": "center", // alias to `background-position`, default `center`
          "interval": 0 // seconds of interval for carousel, default `0` to disabled.
        },

    "terminal.integrated.fontFamily": "MeslolGS Nerd Font",

    "workbench.colorCustomizations": {
        "tab.activeBackground": "#181a0f",
        "activityBar.activeBackground": "#181a1f",
        "activityBar.background": "#21242b",
        "sideBar.background": "#1f2027",
        "editor.background": "#282b33",
    },
}


